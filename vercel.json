{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/ocr",
            "destination": "/api/ocr"
        },
        {
            "source": "/api/ocr/(.*)",
            "destination": "/api/ocr"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}