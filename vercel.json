{
    "name": "vitta-smartquote",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/ocr",
            "destination": "/api/ocr.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}